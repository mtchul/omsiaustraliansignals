{init}
{end}


{frame}
	(L.$.FlashingRoundel) "1" $=
	{if}
		(L.L.Timer) (L.S.Timegap) + (S.L.Timer)  0.5 >
		{if}
			(L.L.RedLight1) (S.L.RedLight2) ! (S.L.RedLight1)
			0 (S.L.Timer)
		{endif}
	{else}
		1 (S.L.RedLight1)
	{endif}

(L.$.DisplayImage) "" $=
	{if}
		"vtss_roundel1.png" (S.$.vtss_image)
	{else}
		"VTSS\" (L.$.DisplayImage) ".png" $+ $+ (S.$.vtss_image)
	{endif}
{end}